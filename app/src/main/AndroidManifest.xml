<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onestardelivery">

    <application
        android:allowBackup="true"
        android:icon="@drawable/onestarlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/onestarsplash"
        android:supportsRtl="true"
        android:theme="@style/Theme.OneStarDelivery">

        <activity android:name=".View.UI.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.UI.SuperShopCategory.SuperShopActivity"/>
        <activity android:name=".View.UI.BakalaCategory.BakalaActivity" />
        <activity android:name=".View.UI.MainActivity"/>

    </application>

</manifest>